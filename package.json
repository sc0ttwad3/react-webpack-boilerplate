{
  "name": "react-webpack-boilerplate",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:js": "babel src --ignore *_tests.js,*_tests-helper.js --out-dir lib --source-maps",
    "build:prod": "webpack --config webpack.config.prod.js --progress",
    "build:dev": "webpack --config webpack.config.dev.js --progress",
    "lint:js": "eslint . --ext .js --ext .jsx --cache",
    "lint:webpack": "webpack-validator webpack.config.dev.js webpack.config.prod.js",
    "lint": "parallelshell \"npm run lint:js\" \"npm run lint:webpack\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Scott Wade",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel": "^6.5.2",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-lodash": "^3.2.5",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.9.2",
    "browser-sync": "^2.13.0",
    "browser-sync-webpack-plugin": "^1.1.0",
    "eslint": "^3.1.1",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-lodash-fp": "^2.0.0",
    "eslint-plugin-react": "^5.2.2",
    "express": "^4.14.0",
    "parallelshell": "^2.0.0",
    "postcss": "^5.1.0",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-validator": "^2.2.3"
  },
  "dependencies": {
    "lodash-fp": "^0.10.4",
    "react": "^15.2.1",
    "react-dom": "^15.2.1"
  }
}
